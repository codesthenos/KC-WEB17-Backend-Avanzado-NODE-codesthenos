<!DOCTYPE html>
<html>
  <%- include('head.ejs') %>
  <body>
    <%- include('header.ejs')%>
    <form method="post" class="form">
      <div>
        <label for="email"><%= __('Email') %></label>
        <input
          type="email"
          id="email"
          name="email"
          value="<%= email %>"
          required
        />
      </div>
      <div>
        <label for="password"><%= __('Password') %></label>
        <input type="password" id="password" name="password" required />
      </div>

      <button class="btn"><%= __('Submit') %></button>
    </form>
    <% if (error) { %>
    <pre><%= __(error) %></pre>
    <% } %>
  </body>
</html>
